package com.vcube.arrays;

public class TwoDimenssionArray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
int [][] a=new int[3][3];
a[0][0]=10;
a[0][1]=20;
a[0][2]=30;

a[1][0]=40;
a[1][1]=50;
a[1][2]=60;

a[2][0]=70;
a[2][1]=80;
a[2][2]=90;
 
int count=0;

for (int[] j : a) {
	count=0;
	for (int k : j) {
		if(count<2) {System.out.print(k+" |");}
		count++;
	}
	System.out.println();
}
	}

}
