package com.vcube.arrays;

public class ReversetheArray {
	public static void main(String[] args) {
		int[] a = { 12, 13, 14, 15 };

		int r = 0;
		int reversed = 0;
		int count = 0;
		int size=a.length;
		while (size-1 >= 0) {

			while (a[count] != 0) {
				
				r=a[count]%10;
				reversed=a[count]*10+r;
				a[count]=a[count]/10;

			}
			a[count]=reversed;
			System.out.println(a[count]+" ");
			count++;
			size--;
//			System.out.println(a);
		}

	}
}
