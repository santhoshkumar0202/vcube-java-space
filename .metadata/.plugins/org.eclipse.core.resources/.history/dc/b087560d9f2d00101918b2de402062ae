package com.Vcubeforloop;

public class Primenumber {

	static boolean isprime(int n) {
		Boolean flage = true;
		for (int i = 2; i < n; i++) {

			if (i == 0 || i == 1) {
				flage = false;

			} else if(n%i==0){
                 
				flage = false;
				 break;
            
			}

		}
		return flage;

	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
System.out.println(isprime(7));
	}

}
