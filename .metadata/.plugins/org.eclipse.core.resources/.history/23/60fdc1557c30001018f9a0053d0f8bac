	boolean isPrime(int i) {
		boolean prime_number=true;
		if(i==0 || i==1) {
			prime_number=false;
		}
		for (int j = 2; j < i; j++) {
			if(i%j==0) {
				
				prime_number=false;
				break;
			};
			
		}
		return prime_number;
	}

