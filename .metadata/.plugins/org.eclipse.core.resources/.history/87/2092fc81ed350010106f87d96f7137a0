package com.Vcubeforloop;

import java.util.Arrays;

public class Reverse {
	public static void main(String[] args) {
		int[] arr = { 1, 2, 3, 4 ,5,6};
reverse(0,arr);
System.out.println(Arrays.toString(arr));
reverse(3,arr);
		

	}
	
static	void reverse(int start,int []arr) {
		
		
		int dec=arr.length-1;
		int  inc=start;
		while (inc<dec) {
			int temp=arr[inc];
			arr[inc]=arr[dec];
				arr[dec]=temp;	
			inc++;
			dec--;
		}
		System.out.println(Arrays.toString(arr));
	}
}
