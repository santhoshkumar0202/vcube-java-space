package com.vcube;

public class TestDemo {

	// we can create in obj class only with static
	// you cant create obj without jvm will not load
//	static TestDemo t1 = new TestDemo();
//def instance block
	
		System.out.println("hello this instance block");
		
	//c block
	static {
		System.out.println("hello this static block");
	}
	{
		System.out.println("hello this instance 02 block");
	}
	
	static TestDemo t1 = new TestDemo();
	
	//main
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		test1();
		test2();
		t1.test3();
		t1.test4();
		System.out.print(t1);
	}

//static method
	static void test1() {
		System.out.println("test one was called ");
	}

	static void test2() {
		System.out.println("test two was called and it is static method ");
	}

// instance method
	void test3() {
		System.out.println("test three was instance called ");
		test2();
	}

	void test4() {
		System.out.println("test fout was called and it is instance method ");
	}
	static {
		System.out.println("hello this static block after main");
	}

}
