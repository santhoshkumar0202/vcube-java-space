package com.Vcubeforloop;

public class Primenumber {

	static boolean isprime(int n) {
		Boolean flage = true;
		for (int i = 2; i < n; i++) {

			if (i == 0 || i == 1) {
				flage = false;

			} else {

				flage = false;

			}

		}
		return flage;

	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
isprime(10);
	}

}
